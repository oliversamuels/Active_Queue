<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GeneSys Health</title>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,300&display=swap" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet">
    <link href="~/css/AboutUs.css" rel="stylesheet">
    <link href="~/css/News.css" rel="stylesheet">
    <link href="~/css/How.css" rel="stylesheet">
</head>
<body>
    <header class="container-fluid  p-0">
        <nav class="navbar navbar-expand-lg bg-gradient">
            <div class="container-fluid px-5">
                <a class="navbar-brand" asp-page="index"><img src="~/assets/images/GHIS_logo.png" alt="GHIS Logo"/></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupport" aria-controls="navbarSupport" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupport">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0 m-auto">
                        <li class="nav-item dropdown bg-transparent">
                            <a class="nav-link dropdown-toggle fw-bold" role="button" data-bs-toggle="dropdown" aria-expanded="false" asp-area="" asp-page="">Product</a>
                            <ul class="dropdown-menu dropdown-menu-transparent bg-gradient">
                                <li><a class="dropdown-item" asp-page="">Product 1</a></li>
                                <li><a class="dropdown-item" asp-page="">Product 2</a></li>
                                <li><hr class="dropdown-divider"></li>
                                <li><a class="dropdown-item" asp-page="">Product 3</a></li>
                                <li><a class="dropdown-item" asp-page="">Product 4</a></li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" asp-page="news">News</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" asp-page="aboutus">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" asp-page="how">How It Works</a>
                        </li>
                    </ul>
                    <ul class="navbar-nav">
                        <li><a class="nav-link navb btn btn-primary p-2" asp-page="">Request Demo</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid section">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer">
        <div class="container p-5">
            <div class="row">
                <div class="col-3">
                    <div>
                        <img src="~/assets/images/GHIS_logo.png" class="img-fluid" alt="" width="150px"/>
                    </div>
                    <div>
                        <div class="d-flex justify-content-start align-items-center my-3">
                            <div class="box">
                                <i class="bi bi-telephone-fill fs-2 fw-bold text-primary"></i>
                            </div>
                            <p>
                                0815 026 0388
                            </p>
                        </div>
                        <div class="d-flex justify-content-start align-items-center my-3">
                            <div class="box">
                                <i class="bi bi-envelope-fill fs-2 fw-bold text-primary"></i>
                            </div>
                            <p>
                                support@genesys-health
                            </p>
                        </div>
                        <div class="d-flex justify-content-start align-items-center my-3">
                            <div class="box">
                                <i class="bi bi-geo-alt-fill fs-2 fw-bold text-primary"></i>
                            </div>
                            <p>
                                 Plot 1187, Abimbola Awoniyi Close, Saka Tinubu St, Victoria Island 100101, Lagos
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-2">
                    <h3 class="mb-3">Sitemap</h3>
                    <p class="mb-3">Genesys EMR</p>
                    <p class="mb-3">Genesys HMIS</p>
                    <p class="mb-3">Genesys ERP</p>
                    <p class="mb-3">Why Genesys?</p>
                    <p class="mb-3">How it works</p>
                </div>
                <div class="col-2">
                    <h3 class="mb-3">Company</h3>
                    <p class="mb-3">About Us</p>
                    <p class="mb-3">Contact Us</p>
                    <p class="mb-3">Products</p>
                    <p class="mb-3">Newsletter</p>
                </div>
                <div class="col-2">
                    <h3 class="mb-3">Legal</h3>
                    <p class="mb-3">Terms</p>
                    <p class="mb-3">Privacy</p>
                    <p class="mb-3">Security</p>
                </div>
            </div>
        </div>
        <div class=" container d-flex justify-content-end align-items-center mb-5">
            <div class="mx-4">
                <img src="https://img.icons8.com/color/48/000000/facebook-new.png" width="25"/>
            </div>
            <div class="mx-4">
                <img src="https://img.icons8.com/color/48/000000/instagram-new--v1.png" width="25"/>
            </div>
            <div class="mx-4">
                <img src="https://img.icons8.com/color/48/000000/linkedin.png" width="25"/>
            </div>
            <div class="mx-4">
                <img src="https://img.icons8.com/color/48/000000/twitter-circled--v1.png" width="25"/>
            </div>
            <div class="mx-4">
                <img src="https://img.icons8.com/color/48/000000/youtube-play.png" width="25"/>
            </div>
        </div>
        <div class="w-75 m-auto d-flex justify-content-between align-items-center border-top p-4">
            <div>
                <p>&copy; Genesys Health 2022. All Rights Reserved.</p>
            </div>
            <div class="d-flex justify-content-between align-items-center">
                <p>Privacy policy</p>
                <p>Terms of use</p>
                <p>Cookie policy</p>
                <p>EMR Intergration</p>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>